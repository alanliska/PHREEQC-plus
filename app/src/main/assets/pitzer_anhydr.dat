	log_k  0.0

	Hfo_w[O][H]  + [H]+ = Hfo_w[O][H]2+
	log_k  7.29    # = pKa1,int

	Hfo_w[O][H] = Hfo_w[O]- + [H]+
	log_k  -8.93   # = -pKa2,int

###############################################
