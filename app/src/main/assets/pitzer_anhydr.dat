	Hfo_w[O][H] + C[O]3-2 + [H]+ = Hfo_wC[O]3- + [H]2[O]
	log_k   12.56

	Hfo_w[O][H] + C[O]3-2 + 2[H]+= Hfo_w[H]C[O]3 + [H]2[O]
	log_k   20.62
